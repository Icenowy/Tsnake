#: src/mode_end.cpp:17
#, c-format
msgid "You win! Welcome to level %d"
msgstr "你赢了！欢迎来到第 %d 关的新世界！"

#: src/mode_end.cpp:19
#, c-format
msgid "You lose. What a pity."
msgstr "你输了。苦命的人儿啊！"

#: src/mode_end.cpp:24
msgid "Press 'p' to play, 'r' to refresh map"
msgstr "按 p 键开始游戏，按 r 键换张地图。"

#: src/mode_end.cpp:25
msgid "or press 'q' to quit"
msgstr "或者按 q 键退出。"

#: src/mode_play.cpp:33
msgid "So hard the wall is!"
msgstr "这墙好硬啊……"

#: src/mode_play.cpp:36
msgid "Destiny cannot be escaped."
msgstr "命运怎是蝼蚁可以逃离的！"

#: src/mode_play.cpp:39
msgid "Why is my body so hard..."
msgstr "我的身体……你为什么这么硬……"

#: src/mode_play.cpp:42
msgid "What the hell is this?!"
msgstr "这是什么鬼？！"

#: src/mode_pause.cpp:12
msgid "PAUSED. Press any key to continue."
msgstr "游戏暂停，按任意键继续"

#: src/game.cpp:26
msgid "***Welcome to Tsnake***"
msgstr "***欢迎来到终端贪吃蛇的世界***"

#: src/game.cpp:27
msgid "To read the handbook, please use --help option"
msgstr "使用 --help 参数来阅读游戏手册"

#: src/game.cpp:64
msgid "Scores: "
msgstr "已得分数："

#: src/game.cpp:68
msgid "Moves: "
msgstr "移动次数："

#: src/mod.cpp:23
msgid "You're trying to play a mod"
msgstr "你启用了一个MOD"

#~ msgid "***Tsnake Version 0.0.3***"
#~ msgstr "***终端贪吃蛇 0.0.3***"

