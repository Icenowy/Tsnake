#: src/mode_end.cpp:17
#, c-format
msgid "You win! Welcome to level %d"
msgstr "你赢了！欢迎来到第 %d 关的新世界！"

#: src/mode_end.cpp:19
#, c-format
msgid "You lose. What a pity."
msgstr "你输了。苦命的人儿啊！"

#: src/mode_end.cpp:24 src/mode_end.cpp:41
msgid "Press 'p' to play, or press 'q' to quit"
msgstr "按 p 键开始游戏，按 q 键退出。"

#: src/mode_end.cpp:40
msgid "Invalid input"
msgstr "输入无效。"

#: src/mode_play.cpp:29
msgid "So hard the wall is!"
msgstr "这墙好硬啊……"

#: src/mode_play.cpp:33
msgid "Destiny cannot be escaped."
msgstr "命运怎是蝼蚁可以逃离的！"

#: src/mode_play.cpp:37
msgid "Why is my body so hard..."
msgstr "我的身体……你为什么这么硬……"

#: src/mode_play.cpp:41
msgid "What the hell is this?!"
msgstr "这是什么鬼？！"

#: src/mode_pause.cpp:12
msgid "PAUSED. Press any key to continue."
msgstr "游戏暂停，按任意键继续"

#: src/game.cpp:21
msgid "***Tsnake Version 0.0.0***"
msgstr "***终端贪吃蛇 0.0.0***"

#: src/game.cpp:22
msgid "To read the handbook, please use --help option"
msgstr "使用 --help 参数来阅读游戏手册"
